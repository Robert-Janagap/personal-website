extends lib/docwrapper

append title-page
    title Robert Janagap

append head-link
    link(rel='stylesheet' href='assets/css/lib/normalize.css')
    link(rel='stylesheet' href='assets/css/lib/boilerplate-style.css')
    link(rel='stylesheet' href='assets/css/lib/font-awesome.css')
    link(rel='stylesheet' href='assets/css/main.css')
append scripts
    script window.jQuery || document.write('<script src="assets/js/lib/jquery-1.11.1.min.js"><\/script>')
    script(src="assets/js/main.js")
append logo
    a(href="")
        img(src="assets/img/logo-2.png" alt="My Logo" style="width: 200px; height: 48px;")
    a(href="").header_logo--fixed
        img(src="assets/img/rr_logo.png" alt="My Logo" style="height: 60px;")
append mainNav-links
    nav.header_nav
        ul.header_list
            li.header_items
                a(href="about/").header_items--link About
            li.header_items
                a(href="blog/").header_items--link Blog
            li.header_items
                a(href="contact/").header_items--link Contact
append responsive-links
